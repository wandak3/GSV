{
  "compilerOptions": {
    "target": "es2021",
    "module": "CommonJS",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "outDir": "./build",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true
  },
 "include":["./src/**/*", "command.ts"] 
}